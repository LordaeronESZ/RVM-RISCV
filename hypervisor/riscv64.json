{
    "arch": "riscv64",
    "code-model": "medium",
    "cpu": "generic-rv64",
    "data-layout": "e-m:e-p64:64:64-i64:64-i128:128-n64-S128",
    "disable-redzone": true,
    "executables": true,
    "features": "+m,+a,+f,+d,+c",
    "linker": "rust-lld",
    "linker-flavor": "ld.lld",
    "llvm-target": "riscv64-unknown-none-elf",
    "max-atomic-width": 64,
    "panic-strategy": "abort",
    "pre-link-args": {
        "ld.lld": [
            "-Tlinker.lds"
        ]
    },
    "relocation-model": "static",
    "target-pointer-width": "64"
}
